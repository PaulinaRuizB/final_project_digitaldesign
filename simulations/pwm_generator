#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a34736d3520 .scope module, "tb_pwm_generator" "tb_pwm_generator" 2 3;
 .timescale -9 -12;
v0x5a34736e87a0_0 .var "clk", 0 0;
v0x5a34736e8890_0 .var "duty", 7 0;
v0x5a34736e8960_0 .var "en", 0 0;
v0x5a34736e8a60_0 .net "pwm_out", 0 0, v0x5a34736e8450_0;  1 drivers
v0x5a34736e8b30_0 .var "rst", 0 0;
S_0x5a34736d36b0 .scope module, "dut" "pwm_generator" 2 13, 3 1 0, S_0x5a34736d3520;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "en";
    .port_info 3 /INPUT 8 "duty";
    .port_info 4 /OUTPUT 1 "pwm_out";
P_0x5a34736d3890 .param/l "PWM_PERIOD" 1 3 10, +C4<00000000000000000000100111000100>;
v0x5a3473697490_0 .net "clk", 0 0, v0x5a34736e87a0_0;  1 drivers
v0x5a34736e81c0_0 .var "counter", 11 0;
v0x5a34736e82a0_0 .net "duty", 7 0, v0x5a34736e8890_0;  1 drivers
v0x5a34736e8390_0 .net "en", 0 0, v0x5a34736e8960_0;  1 drivers
v0x5a34736e8450_0 .var "pwm_out", 0 0;
v0x5a34736e8560_0 .net "rst", 0 0, v0x5a34736e8b30_0;  1 drivers
v0x5a34736e8620_0 .var "threshold", 11 0;
E_0x5a3473697d90 .event posedge, v0x5a34736e8560_0, v0x5a3473697490_0;
    .scope S_0x5a34736d36b0;
T_0 ;
    %wait E_0x5a3473697d90;
    %load/vec4 v0x5a34736e8560_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x5a34736e81c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a34736e8450_0, 0;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x5a34736e8620_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5a34736e8390_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5a34736e81c0_0;
    %pad/u 32;
    %cmpi/u 2499, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x5a34736e81c0_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5a34736e81c0_0;
    %addi 1, 0, 12;
    %assign/vec4 v0x5a34736e81c0_0, 0;
T_0.5 ;
    %load/vec4 v0x5a34736e82a0_0;
    %pad/u 32;
    %muli 2500, 0, 32;
    %ix/load 4, 8, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %pad/u 12;
    %assign/vec4 v0x5a34736e8620_0, 0;
    %load/vec4 v0x5a34736e81c0_0;
    %load/vec4 v0x5a34736e8620_0;
    %cmp/u;
    %flag_get/vec4 5;
    %assign/vec4 v0x5a34736e8450_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x5a34736e81c0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a34736e8450_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5a34736d3520;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a34736e87a0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5a34736d3520;
T_2 ;
    %delay 10000, 0;
    %load/vec4 v0x5a34736e87a0_0;
    %inv;
    %store/vec4 v0x5a34736e87a0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5a34736d3520;
T_3 ;
    %vpi_call 2 28 "$dumpfile", "pwm_generator_tb.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a34736d3520 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a34736e8b30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a34736e8960_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5a34736e8890_0, 0, 8;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a34736e8b30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a34736e8960_0, 0, 1;
    %pushi/vec4 64, 0, 8;
    %store/vec4 v0x5a34736e8890_0, 0, 8;
    %delay 500000000, 0;
    %pushi/vec4 128, 0, 8;
    %store/vec4 v0x5a34736e8890_0, 0, 8;
    %delay 500000000, 0;
    %pushi/vec4 192, 0, 8;
    %store/vec4 v0x5a34736e8890_0, 0, 8;
    %delay 500000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a34736e8960_0, 0, 1;
    %delay 50000000, 0;
    %vpi_call 2 56 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
